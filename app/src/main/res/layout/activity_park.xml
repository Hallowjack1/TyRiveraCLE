<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#6AC2CC"
    tools:context=".park"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView15"
        android:layout_width="300dp"
        android:layout_height="67dp"
        android:text="@string/central_park"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="48sp"
        android:translationX="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginBottom="328dp"
        android:contentDescription="@string/central_park"
        android:translationX="45dp"
        android:translationY="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView15"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/centralpark" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="352dp"
        android:layout_height="310dp"
        android:justificationMode="inter_word"
        android:text="@string/central_park_content"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:translationX="20dp"
        android:translationY="-300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

</LinearLayout>